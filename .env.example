# Configuration Pretix Sortir! Plugin
# Copier ce fichier en .env et remplir les valeurs

# Token API APRAS (fourni par l'APRAS)
SORTIR_API_TOKEN=your_token_here

# Clé de chiffrement Fernet pour sécuriser les données sensibles
# Générer avec : python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
SORTIR_ENCRYPTION_KEY=generate_your_key_here

# Configuration Django (si nécessaire)
DEBUG=False
SECRET_KEY=your_django_secret_key_here